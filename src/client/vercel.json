{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "vite",
  "routes": [
    { "src": "/api/(.*)", "dest": "https://queuedrop-production.up.railway.app/api/$1" },
    { "src": "/hubs/(.*)", "dest": "https://queuedrop-production.up.railway.app/hubs/$1" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
